{
	"manifest_version": 2,
	"name": "快速阅读",
	"version": "1.0",
	"description": "快速阅读",
	"author": "Q",
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"browser_action": 
	{
		"default_icon": "img/icon.png",
		"default_title": "quick reading 快速阅读",
		"default_popup": "main.html"
	},
	"background": 
	{
		"page": "back.html"
	},
	"permissions":
	[
		"tabs",
		"*://*/*",
		"http://*/*",
		"https://*/*"
	]
}